{
  "name": "nocode-starter-backend",
  "version": "0.1.0",
  "private": false,
  "main": "./dist/run.js",
  "types": "./dist/run.d.ts",
  "exports": {
    ".": "./dist/run.js"
  },
  "bin": {
    "nocode-starter-backend": "dist/run.js"
  },
  "tsup": {
    "tsconfig": "./tsconfig.run.json"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build:run": "tsup run.ts --dts-resolve",
    "build:nest": "nest build",
    "build": "pnpm build:nest && pnpm build:run",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev:run": "tsup run.ts --watch --onSuccess \"node dist/run.js\"",
    "dev": "nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash.set": "^4.3.2",
    "mongodb": "^4.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^6.1.0",
    "jest": "^27.2.5",
    "nocode-starter-core": "workspace:*",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "tsup": "^5.12.4",
    "typescript": "^4.3.5"
  }
}
