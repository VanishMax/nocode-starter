{
  "name": "@nocode/server",
  "description": "No-code platform server: API for creating projects and managing them",
  "version": "0.1.0",
  "main": "./dist/run.js",
  "types": "./dist/run.d.ts",
  "exports": {
    ".": "./dist/run.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "nocode-backend": "dist/run.js"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build:run": "tsup run.ts --dts-resolve",
    "build:nest": "nest build",
    "build": "pnpm build:nest && pnpm build:run",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev:run": "tsup run.ts --watch --onSuccess \"node dist/run.js\"",
    "dev": "nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "tsup": {
    "tsconfig": "./tsconfig.run.json"
  },
  "dependencies": {
    "@nestjs/common": "9.0.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.0",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/swagger": "6.0.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.set": "^4.3.2",
    "mongodb": "^4.8.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.9",
    "@nocode/core": "workspace:0.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.set": "^4.3.7",
    "@types/node": "18.7.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^6.1.0",
    "jest": "28.1.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "tsup": "6.2.1",
    "typescript": "^4.7.4"
  }
}
