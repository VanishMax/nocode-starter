{
  "name": "nocode-starter-ui",
  "version": "0.1.0",
  "private": false,
  "main": "./dist/run.js",
  "types": "./dist/run.d.ts",
  "exports": {
    ".": "./dist/run.js"
  },
  "scripts": {
    "dev": "vite",
    "dev:run": "tsup run.ts --watch --onSuccess \"node dist/run.js\"",
    "build:run": "tsup run.ts --dts-resolve",
    "build:vite": "vite build",
    "build": "pnpm build:bite && pnpm build:run",
    "lint": "eslint --fix src"
  },
  "tsup": {
    "tsconfig": "./tsconfig.run.json",
    "external": ["vite"]
  },
  "bin": {
    "nocode-starter-ui": "dist/run.js"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.6",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "pinia": "^2.0.11",
    "uuid": "^8.3.2",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^16.0.0",
    "@types/tailwindcss": "^3.0.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.4.0",
    "execa": "^6.1.0",
    "nocode-starter-core": "workspace:*",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.18",
    "tsup": "^5.12.4",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
