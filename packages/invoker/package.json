{
  "name": "nocode-starter-invoker",
  "private": true,
  "version": "0.1.0",
  "main": "./dist/run.js",
  "types": "./dist/run.d.ts",
  "exports": {
    ".": "./dist/run.js"
  },
  "scripts": {
    "dev": "vite",
    "build:run": "tsup run.ts --dts-resolve",
    "build:vite": "vite build",
    "build": "pnpm build:vite && pnpm build:run",
    "start": "vite preview"
  },
  "tsup": {
    "tsconfig": "./tsconfig.run.json",
    "external": ["vite"]
  },
  "bin": {
    "nocode-starter-invoker": "dist/run.js"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "execa": "^6.1.0",
    "nocode-starter-core": "workspace:*",
    "tsup": "^5.12.4",
    "typescript": "^4.5.4",
    "vite": "^2.9.2",
    "vue-tsc": "^0.29.8"
  }
}
