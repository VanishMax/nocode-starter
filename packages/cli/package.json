{
  "name": "nocode-starter-cli",
  "description": "CLI for running no-code starter inside the app directory",
  "version": "0.1.0",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsup index.ts --dts-resolve",
    "dev": "tsup index.ts --onSuccess \"node dist/index.js start\""
  },
  "tsup": {
    "external": [
      "esbuild"
    ]
  },
  "bin": {
    "nocode-starter": "dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/listr": "^0.14.4",
    "@types/node": "^16.0.0",
    "chalk": "^5.0.1",
    "commander": "^9.1.0",
    "esbuild": "^0.14.36",
    "execa": "^6.1.0",
    "listr": "^0.14.3",
    "nocode-starter-backend": "workspace:*",
    "nocode-starter-invoker": "workspace:*",
    "nocode-starter-ui": "workspace:*",
    "ts-node": "10.7.0",
    "tsup": "^5.12.4",
    "typescript": "4.6.3",
    "vite": "^2.7.2"
  },
  "keywords": [
    "no-code",
    "cli",
    "no-code boilerplate"
  ],
  "author": "Max Korsunov (https://vanishmax.com/)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanishmax/nocode-starter.git"
  },
  "bugs": {
    "url": "https://github.com/vanishmax/nocode-starter/issues"
  }
}
